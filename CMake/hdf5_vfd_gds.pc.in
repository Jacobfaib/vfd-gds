# This gives access to the HDF5 GDS VFD header files
prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=@CMAKE_INSTALL_PREFIX@
libdir=@HDF5_VFD_GDS_INSTALL_LIB_DIR@
includedir=@HDF5_VFD_GDS_INSTALL_INCLUDE_DIR@

Name: @HDF5_VFD_GDS_PACKAGE@
Description: @HDF5_VFD_GDS_PACKAGE_DESCRIPTION@
Version: @HDF5_VFD_GDS_VERSION_MAJOR@.@HDF5_VFD_GDS_VERSION_MINOR@.@HDF5_VFD_GDS_VERSION_PATCH@
URL: @HDF5_VFD_GDS_PACKAGE_URL@
Requires: @HDF5_VFD_GDS_PKG_DEPENDENCIES@
Libs: -L${libdir} @HDF5_VFD_GDS_LIBRARIES@
Libs.private: @HDF5_VFD_GDS_LIB_DEPENDENCIES@
Cflags: -I${includedir} @HDF5_VFD_GDS_INCLUDE_DEPENDENCIES@
